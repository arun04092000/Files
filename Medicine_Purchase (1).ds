			On_Validation_Edit_vendor as "On Validation - Edit vendor"
			{
				type =  form
				form = Medicine_Vendor_Setup
				record event = on edit

				on validate
				{
					actions 
					{
						custom deluge script
						(
											if(Vendor_Name.Vendor_Name != Vendor_Name_Fetch)
										{
											fet = Medicine_Vendor_Setup[Medicine_Name == input.Medicine_Name || Vendor_Name == input.Vendor_Name];
											if(fet.count() > 0)
											{
												alert "Already the Medicine has been assigned to Vendor: " + input.Vendor_Name_Fetch;
												cancel submit;
											}
										}
										else if(Medicine_Name.Medicine_Name != Medicine_Name_Fetch)
										{
											alert "Invalid Medicine";
											cancel submit;
										}
						)
					}
				}

			}
