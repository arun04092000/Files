			Status_completed as "Status completed"
			{
				type =  form
				form = Received_Medicine
				record event = on add or edit

				on user input of Oder_details.Completed
				{
					actions 
					{
						custom deluge script
						(
											lis = list();
										for each  rec in input.Oder_details
										{
											lis.add(rec.Completed);
										}
										if(lis.contains("No"))
										{
											input.Available_status = "No";
										}
										else
										{
											input.Available_status = "Yes";
										}
						)
